{
    "manifest_version": 3,
    "name": "URL to Google Doc Appender",
    "version": "1.0.1",
    "permissions": [
        "activeTab",
        "storage",
        "identity",
        "tabs"
    ],
    "host_permissions": [
        "https://docs.googleapis.com/"
    ],
    "action": {
        "default_popup": "popup.html"
    },
    "background": {
        "service_worker": "background.js"
    },
    "options_page": "options.html",
    "key": "",
    "oauth2": {
        "client_id": "126727248936-4k4dt231jabf9p7lt798d4ojgbdn5ifr.apps.googleusercontent.com",
        "scopes": [
            "https://www.googleapis.com/auth/documents"
        ]
    },
    "commands": {
        "append_url": {
            "suggested_key": {
                "default": "Ctrl+Shift+U",
                "mac": "Command+Shift+U"
            },
            "description": "Append current URL to Google Doc"
        }
    },
    "icons": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
    }
}